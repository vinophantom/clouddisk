<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "../dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.phantom.entity.UserFolder" table="user_folder" schema="clouddisk">
        <id name="folderId" column="folder_id">
            <generator class="increment" />
        </id>
        <property name="userId" column="user_id"/>
        <property name="parentId" column="parent_id"/>
        <property name="folderName" column="folder_name"/>
        <property name="createTime" column="create_time"/>
        <property name="modifyTime" column="modify_time"/>
        <property name="deleteTime" column="delete_time"/>
    </class>
</hibernate-mapping>